<Window x:Class="BalansKolorow.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="139" Width="386">
    <Grid Margin="0,0,10,0" Height="107">
        <Button x:Name="przycisk1" Content="Launch C++" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="54" Width="104" Click="LaunchCPP"/>
        <Button x:Name="przycisk2" Content="Launch ASM" HorizontalAlignment="Left" Margin="132,10,0,0" VerticalAlignment="Top" Height="54" Width="104" Click="LauchASM"/>
        <Button x:Name="przycisk3" Content="Hello World" Margin="254,10,16,0" VerticalAlignment="Top" Height="54" Click="LaunchHelloWorld"/>
        <TextBox x:Name="textbox" Margin="10,75,10,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top"/>

    </Grid>
</Window>
